{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++ Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/RPN",
      "args": ["9", "21", "20", "7", "11", "17", "19", "13", "18", "22", "14"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build",
      "miDebuggerPath": "/usr/bin/gdb", // gdbのパスを指定
      "logging": {
        "trace": true,
        "traceResponse": true,
        "engineLogging": true
      },
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}

